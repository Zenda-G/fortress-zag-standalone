{
  "name": "fortress-zag-standalone",
  "version": "4.3.0",
  "description": "Fully autonomous AI assistant v4.3 - Verification, Checkpoints, Continuous Eval, Memory Dashboard, Task Scheduler V2, Bat-Gadget Protocol, Git-backed memory, cloud compute",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "cli": "node src/interfaces/cli.js",
    "test": "node test-full.js",
    "test:v4": "node test-v4.js",
    "test:bgp": "node test-bgp.js",
    "test:p1": "node test-p1.js",
    "test:p2": "node test-p2.js",
    "install:browser": "npx playwright install chromium",
    "docker:build": "docker build -t fortress-zag .",
    "docker:run": "docker run -v $(pwd)/data:/app/data -p 3000:3000 fortress-zag",
    "git:init": "git init && git add -A && git commit -m 'Initial commit'",
    "setup": "node setup/setup.js"
  },
  "keywords": [
    "ai",
    "assistant",
    "autonomous",
    "secure",
    "standalone",
    "browser",
    "scheduler",
    "memory-dashboard",
    "task-scheduler",
    "git-backed-memory",
    "cloud-compute",
    "bat-gadget-protocol"
  ],
  "author": "Zak & Zag",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "discord.js": "^14.14.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.64.0",
    "playwright": "^1.40.0",
    "uuid": "^9.0.0",
    "yaml": "^2.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}